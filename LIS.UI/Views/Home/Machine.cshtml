
@{
    ViewBag.Title = "Machine";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="frmmachine">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Manage Machine </div>
                    </div>
                    <div class="card-body col-md-7">
                        <div class="form-group">
                            <label for="txtmachinename">Machine Name</label>
                            <input type="text" class="form-control" id="txtmachinename" name="txtmachinename" />
                        </div>

                        <div class="form-group">
                            <label for="filemachine" class=" form-control-label">template</label>
                            <input type="file" id="filemachine" name="filemachine" class="form-control-file" />
                        </div>


                        <div class="form-group">
                            <input type="button" id="btnmachinesave" name="btnmachinesave" class="btn btn-success" value="Save" />
                            <input type="button" name="btnmachinecancel" class="btn btn-default" value="Cancel" />
                        </div>
                    </div>

                    <div class="card-footer">

                    </div>



                </div>

            </div>
        </div>
    </div>

</form>
