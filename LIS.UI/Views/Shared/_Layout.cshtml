    <script>
        document.onreadystatechange = function () {
            var state = document.readyState
            if (state == 'interactive') {
                jQuery("#bodycontainer").addClass("loader-backgroud");
                //document.getElementById('bodycontainer').style.visibility = "hidden";
            } else if (state == 'complete') {
                setTimeout(function () {
                    document.getElementById('interactive');
                    document.getElementById('load').style.visibility = "hidden";
                    //   document.getElementById('bodycontainer').style.visibility = "visible";
                    jQuery("#bodycontainer").removeClass("loader-backgroud");
                }, 1000);
            }
        }
        jQuery(".dataTables_paginate").css('float', 'right');
    </script>


